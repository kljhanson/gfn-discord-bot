    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "runtimeVersion": "16.17.0",
            "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v16.17.0/bin/node",
            "outputCapture": "std",
            "request": "launch",
            "name": "GFN Bot Debug",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/bot.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}